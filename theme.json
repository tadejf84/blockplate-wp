{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "768px",
			"wideSize": "1280px"
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Roboto\"",
					"name": "Roboto",
					"slug": "base",
					"fontFace": [
						{
							"fontFamily": "\"Roboto\"",
							"fontStyle": "normal",
							"fontWeight": "300 400 500 600 700 800",
							"src": [
								"file:./assets/fonts/Roboto-Regular.woff2"
							]
						},
						{
							"fontFamily": "\"Roboto\"",
							"fontStyle": "italic",
							"fontWeight": "300 400 500 600 700 800",
							"src": [
								"file:./assets/fonts/Roboto-Italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Base",
					"size": "1rem",
					"slug": "base"
				},
				{
					"name": "Heading 6",
					"size": "1.333rem",
					"slug": "heading-6",
					"fluid": {
						"max": "1.333rem",
						"min": "1.2rem"
					}
				},
				{
					"name": "Heading 5",
					"size": "1.777rem",
					"slug": "heading-5",
					"fluid": {
						"max": "1.777rem",
						"min": "1.44rem"
					}
				},
				{
					"name": "Heading 4",
					"size": "2.369rem",
					"slug": "heading-4",
					"fluid": {
						"max": "2.369rem",
						"min": "1.728rem"
					}
				},
				{
					"name": "Heading 3",
					"size": "3.157rem",
					"slug": "heading-3",
					"fluid": {
						"max": "3.157rem",
						"min": "2.074rem"
					}
				},
				{
					"name": "Heading 2",
					"size": "4.209rem",
					"slug": "heading-2",
					"fluid": {
						"max": "4.209rem",
						"min": "2.488rem"
					}
				},
				{
					"name": "Heading 1",
					"size": "5.61rem",
					"slug": "heading-1",
					"fluid": {
						"max": "5.61rem",
						"min": "2.986rem"
					}
				}
			]
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#FDFDFD",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000",
					"name": "Main",
					"slug": "main"
				},
				{
					"color": "#B5B8C0",
					"name": "Neutral",
					"slug": "neutral"
				},
				{
					"color": "#0A2A43",
					"name": "Dark",
					"slug": "dark"
				},
				{
					"color": "#FF6F5E",
					"name": "Accent",
					"slug": "accent"
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Small",
					"size": "clamp(.5rem, 2.5vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 4vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 5vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(3rem, 7vw, 5rem)",
					"slug": "x-large"
				},
				{
					"name": "2xl",
					"size": "clamp(4rem, 9vw, 7rem)",
					"slug": "xx-large"
				},
				{
					"name": "3xl",
					"size": "clamp(5rem, 12vw, 9rem)",
					"slug": "xxx-large"
				},
				{
					"name": "4xl",
					"size": "clamp(6rem, 14vw, 13rem)",
					"slug": "xxxx-large"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"border": {
			"radiusSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "var(--wp--custom--border-radius--small)"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "var(--wp--custom--border-radius--medium)"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "var(--wp--custom--border-radius--large)"
				},
				{
					"name": "Round",
					"slug": "round",
					"size": "var(--wp--custom--border-radius--round)"
				}
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Very light",
					"shadow": "0 1px 3px rgba(0,0,0,0.12), 0 1px 2px rgba(0,0,0,0.24)",
					"slug": "very-light"
				},
				{
					"name": "Light",
					"shadow": "0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23)",
					"slug": "light"
				},
				{
					"name": "Medium",
					"shadow": "0 10px 20px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23)",
					"slug": "medium"
				},
				{
					"name": "Heavy",
					"shadow": "0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22)",
					"slug": "heavy"
				},
				{
					"name": "Very Heavy",
					"shadow": "0 19px 38px rgba(0,0,0,0.30), 0 15px 12px rgba(0,0,0,0.22)",
					"slug": "very-heavy"
				}
			]
		},
		"custom": {
			"borderRadius": {
				"small": "0.5rem",
				"medium": "1rem",
				"large": "2rem",
				"round": "100%"
			},
			"fontWeight": {
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800
			},
			"lineHeight": {
				"base": 1.6,
				"headings": 1.1
			}
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--base)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "var(--wp--custom--line-height--body)"
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--main)"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--medium)",
			"padding": {
				"bottom": "0",
				"left": "var(--wp--preset--spacing--medium)",
				"right": "var(--wp--preset--spacing--medium)",
				"top": "0"
			}
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--base)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				"border": {
					"radius": "5px",
					"width": "0"
				},
				"color": {
					"background": "#244d9e00",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": ".5em",
						"left": "1em",
						"right": "1em",
						"top": ".5em"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)",
					"lineHeight": "var(--wp--custom--line-height--headings)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent)"
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"blocks": {
			"core/button": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"color": "",
					"left": {
						"style": "none",
						"width": "0px"
					},
					"radius": "50px",
					"right": {
						"style": "none",
						"width": "0px"
					},
					"style": "",
					"top": {
						"style": "none",
						"width": "0px"
					},
					"width": ""
				},
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "",
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--large)",
						"right": "var(--wp--preset--spacing--large)",
						"top": "var(--wp--preset--spacing--small)"
					}
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "var(--wp--preset--color--main)",
								"style": "solid",
								"width": "1px"
							},
							"left": {
								"color": "var(--wp--preset--color--main)",
								"style": "solid",
								"width": "1px"
							},
							"right": {
								"color": "var(--wp--preset--color--main)",
								"style": "solid",
								"width": "1px"
							},
							"top": {
								"color": "var(--wp--preset--color--main)",
								"style": "solid",
								"width": "1px"
							}
						},
						"spacing": {
							"padding": {
								"bottom": "var(--wp--preset--spacing--small)",
								"left": "var(--wp--preset--spacing--large)",
								"right": "var(--wp--preset--spacing--large)",
								"top": "var(--wp--preset--spacing--small)"
							}
						}
					}
				}
			},
			"core/code": {
				"border": {
					"radius": "5px",
					"width": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--main)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"top": "var(--wp--preset--spacing--medium)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"top": {
						"style": "solid",
						"width": "5px"
					}
				},
				"elements": {
					"cite": {
						"color": {
							"text": "var(--wp--preset--color--secondary)"
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--font-weight--medium)",
					"lineHeight": "var(--wp--custom--line-height--body)"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"radius": "0",
					"style": "solid",
					"width": "0 0 0 5px !important"
				},
				"css": "& :where(cite) { display: block }",
				"elements": {
					"cite": {
						"color": {
							"text": "var(--wp--preset--color--secondary)"
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--large)",
						"right": "var(--wp--preset--spacing--large)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--font-weight--medium)",
					"lineHeight": "var(--wp--custom--line-height--body)"
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"customTemplates": [
		{
			"name": "page-no-title",
			"postTypes": [
				"page"
			],
			"title": "Page Template (No Title)"
		}
	]
}